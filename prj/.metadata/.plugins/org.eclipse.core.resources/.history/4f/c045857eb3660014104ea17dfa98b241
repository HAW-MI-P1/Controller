 *                              Class Definition                              *
 *****************************************************************************/

public class Controller implements ISearch4Facebook
{
	
/******************************************************************************
 *                                  Fields                                    *
 *****************************************************************************/
	
    public IDBController dbController;
    public IFacebookAPI  facebookAPI;
    public IParser       parser;
    
/******************************************************************************
 *                         Construction & Initialization                      *
 *****************************************************************************/

public Controller(IDBController dbController, IFacebookAPI facebookAPI,
		IParser parser) {
	super();
	this.dbController = dbController;
	this.facebookAPI = facebookAPI;
	this.parser = parser;
}

/******************************************************************************
 *                              Public Methods                                *
 *****************************************************************************/
	
    public Collection<Person> search(String naturalLanguage)
    {
        throw new UnsupportedOperationException();
    }

